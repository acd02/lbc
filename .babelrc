{
  "env": {
    "production": {
      "plugins": [["emotion"]]
    },
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true,
            "autoLabel": true,
            "labelFormat": "[local]",
            "cssPropOptimization": true
          }
        ]
      ]
    }
  }
}
