{
  "name": "poi-playground",
  "version": "1.0.0",
  "main": "src/app.tsx",
  "license": "MIT",
  "scripts": {
    "start": "poi --serve --config build/poi.config.js",
    "build": "npm run lint-prod && poi --prod --config ./build/poi.config.js && netlify-lambda build serverless",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint-prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "serverless": "netlify-lambda serve serverless --port 4001"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "devDependencies": {
    "@poi/plugin-eslint": "12.0.0",
    "@poi/plugin-typescript": "12.2.1",
    "@reach/router": "1.2.1",
    "@types/aws-lambda": "8.10.31",
    "@types/classnames": "2.2.9",
    "@types/node": "12.6.8",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-transition-group": "2.9.2",
    "@types/react-toastify": "4.0.2",
    "@types/yup": "0.26.22",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "autoprefixer": "9.6.1",
    "babel-plugin-emotion": "10.0.14",
    "classnames": "2.2.6",
    "cssnano": "4.1.10",
    "dayjs": "1.8.15",
    "dotenv": "8.0.0",
    "emotion": "10.0.14",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-standard": "4.0.0",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "fp-ts-local-storage": "0.2.0",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "monocle-ts": "2.0.0",
    "netlify-lambda": "1.5.0",
    "object-assign": "4.1.1",
    "poi": "12.7.0",
    "polished": "3.4.1",
    "prettier": "1.18.2",
    "promise-polyfill": "8.1.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-transition-group": "4.2.1",
    "react-toastify": "5.3.2",
    "typescript": "3.5.3",
    "yup": "0.27.0"
  }
}
